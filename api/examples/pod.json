{
  "kind": "Pod",
  "apiVersion": "v1beta1",
  "id": "php",
  "desiredState": {
    "manifest": {
      "version": "v1beta1",
      "id": "php",
      "containers": [{
        "name": "nginx",
        "image": "/home/yifan/gopher/src/playground/loop.aci",
        "ports": [{
          "containerPort": 80,
          "hostPort": 8081
        }],
        "resources": {
          "limits": {
            "memory": "1000",
          },
        },
        "livenessProbe": {
          "enabled": true,
          "type": "http",
          "initialDelaySeconds": 30,
          "httpGet": {
            "path": "/index.html",
            "port": "8081"
          }
        }
      }]
    }
  },
  "labels": {
    "name": "foo"
  }
}

